<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{% block title %}Administration Aamv{% endblock %}</title>
    {% block meta %}{% endblock %}
    {% block stylesheets %}
        {% stylesheets
            'bundles/aamvadmin/assets/bootstrap/css/bootstrap.min.css'
            'bundles/aamvadmin/assets/font-awesome/css/font-awesome.min.css'
            'bundles/aamvadmin/css/flaty.css'
            'bundles/aamvadmin/css/flaty-responsive.css'
        filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endblock %}
{% block specific_stylesheets %}{% endblock %}

<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
    <!-- BEGIN Theme Setting -->
    <div id="theme-setting">
        <a href="#"><i class="fa fa-gears fa fa-2x"></i></a>
        <ul>
            <li>
                <span>Skin</span>
                <ul class="colors" data-target="body" data-prefix="skin-">
                    <li class="active"><a class="blue" href="#"></a></li>
                    <li><a class="red" href="#"></a></li>
                    <li><a class="green" href="#"></a></li>
                    <li><a class="orange" href="#"></a></li>
                    <li><a class="yellow" href="#"></a></li>
                    <li><a class="pink" href="#"></a></li>
                    <li><a class="magenta" href="#"></a></li>
                    <li><a class="gray" href="#"></a></li>
                    <li><a class="black" href="#"></a></li>
                </ul>
            </li>
            <li>
                <span>Navbar</span>
                <ul class="colors" data-target="#navbar" data-prefix="navbar-">
                    <li class="active"><a class="blue" href="#"></a></li>
                    <li><a class="red" href="#"></a></li>
                    <li><a class="green" href="#"></a></li>
                    <li><a class="orange" href="#"></a></li>
                    <li><a class="yellow" href="#"></a></li>
                    <li><a class="pink" href="#"></a></li>
                    <li><a class="magenta" href="#"></a></li>
                    <li><a class="gray" href="#"></a></li>
                    <li><a class="black" href="#"></a></li>
                </ul>
            </li>
            <li>
                <span>Sidebar</span>
                <ul class="colors" data-target="#main-container" data-prefix="sidebar-">
                    <li class="active"><a class="blue" href="#"></a></li>
                    <li><a class="red" href="#"></a></li>
                    <li><a class="green" href="#"></a></li>
                    <li><a class="orange" href="#"></a></li>
                    <li><a class="yellow" href="#"></a></li>
                    <li><a class="pink" href="#"></a></li>
                    <li><a class="magenta" href="#"></a></li>
                    <li><a class="gray" href="#"></a></li>
                    <li><a class="black" href="#"></a></li>
                </ul>
            </li>
            <li>
                <span></span>
                <a data-target="navbar" href="#"><i class="fa fa-square-o"></i> Fixed Navbar</a>
                <a class="hidden-inline-xs" data-target="sidebar" href="#"><i class="fa fa-square-o"></i> Fixed Sidebar</a>
            </li>
        </ul>
    </div>
    <!-- END Theme Setting -->

    {% block navbar %}
        {%  include('AamvAdminBundle::navbar.html.twig') %}
    {% endblock %}

    <!-- BEGIN Container -->
    <div class="container" id="main-container">
        {% block sidebar %}
            {% include('AamvAdminBundle::sidebar.html.twig') %}
        {% endblock %}

        <!-- BEGIN Content -->
        <div id="main-content">
            <!-- BEGIN Page Title -->
            <div class="page-title">
                <div>
                    {% block page_title %}
                        <h1><i class="fa fa-file-o"></i> Dashboard</h1>
                        <h4>Overview, stats, chat and more</h4>
                    {% endblock %}
                </div>
            </div>
            <!-- END Page Title -->

            <!-- BEGIN Breadcrumb -->
            <div id="breadcrumbs">
                <ul class="breadcrumb">
                    <li class="active"><i class="fa fa-home"></i> Home</li>
                </ul>
            </div>
            <!-- END Breadcrumb -->

            {% for flashMessage in app.session.flashbag.get('notice') %}
                <div class="alert alert-info" role="alert">
                    <button class="close" data-dismiss="alert">×</button>
                    <strong>Info!</strong> {{ flashMessage }}
                </div>
            {% endfor %}

            {% for flashMessage in app.session.flashbag.get('error') %}
                <div class="alert alert-danger" role="alert">
                    <button class="close" data-dismiss="alert">×</button>
                    <strong>Error!</strong> {{ flashMessage }}
                </div>
            {% endfor %}

        {% block body %}{% endblock %}

        <footer>
            {% block footer %}
                {% include('AamvAdminBundle::footer.html.twig') %}
            {% endblock %}
        </footer>

        <a id="btn-scrollup" class="btn btn-circle btn-lg" href="#"><i class="fa fa-chevron-up"></i></a>
    </div>
    <!-- END Content -->

</div>
<!-- END Container -->

<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="bundles/aamvadmin/assets/jquery/jquery-2.1.1.min.js"><\/script>')</script>
{% javascripts
    'bundles/aamvadmin/assets/bootstrap/js/bootstrap.min.js'

    'bundles/aamvadmin/assets/jquery-slimscroll/jquery.slimscroll.min.js'
    'bundles/aamvadmin/assets/jquery-cookie/jquery.cookie.js'

    'bundles/aamvadmin/assets/flot/jquery.flot.js'
    'bundles/aamvadmin/assets/flot/jquery.flot.resize.js'
    'bundles/aamvadmin/assets/flot/jquery.flot.pie.js'
    'bundles/aamvadmin/assets/flot/jquery.flot.stack.js'
    'bundles/aamvadmin/assets/flot/jquery.flot.crosshair.js'

    'bundles/aamvadmin/assets/sparkline/jquery.sparkline.min.js'

    'bundles/aamvadmin/js/flaty.js'
    'bundles/aamvadmin/js/flaty-demo-codes.js'
%}
<script src="{{ asset_url }}" type="text/javascript"></script>
{% endjavascripts %}

{% block javascripts %}{% endblock %}
{% block specific_javascripts %}{% endblock %}

</body>
</html>

